<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data/>
    <FrameLayout android:id="@+id/frameCard"
        android:layout_height="wrap_content"
        android:layout_width="match_parent" style="@style/groupStyle_1">
        <ImageView android:id="@+id/imageUnsplashwmyE5I"
            android:layout_gravity="start"
            android:layout_height="@dimen/_60pxh"
            android:layout_width="@dimen/_320pxh"
            android:scaleType="fitXY" cornerRadius="@{@dimen/_19pxh}"
            imageRes="@{@drawable/img_unsplashwmye5i}"
            tools:ignore="ContentDescription" tools:src="@drawable/img_unsplashwmye5i"/>
        <LinearLayout android:id="@+id/linearRowtrash"
            android:layout_gravity="bottom|start"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_16pxv"
            android:layout_marginEnd="@dimen/_20pxh"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_marginTop="@dimen/_16pxv"
            android:layout_width="match_parent" android:orientation="horizontal">
            <ImageView android:id="@+id/imageTrash"
                android:layout_height="@dimen/_17pxh"
                android:layout_marginBottom="@dimen/_4pxv"
                android:layout_marginTop="@dimen/_1pxv"
                android:layout_width="@dimen/_19pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_trash"
                tools:ignore="ContentDescription" tools:src="@drawable/img_trash"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtTimKring"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_13pxh"
                android:layout_marginTop="@dimen/_1pxv"
                android:layout_weight="0.4" android:layout_width="0dp"
                android:letterSpacing="0.03"
                android:lineSpacingExtra="@dimen/_5pxh"
                android:singleLine="true"
                android:text="@string/lbl_tim_kring"
                style="@style/txtSourceSansProsemibold17" tools:text="@string/lbl_tim_kring"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtLanguageOne"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_1pxv"
                android:layout_marginStart="@dimen/_45pxh"
                android:layout_weight="0.5" android:layout_width="0dp"
                android:letterSpacing="0.03"
                android:lineSpacingExtra="@dimen/_5pxh"
                android:singleLine="true"
                android:text="@string/lbl_14_30_04_13"
                style="@style/txtSourceSansProsemibold17" tools:text="@string/lbl_14_30_04_13"/>
        </LinearLayout>
    </FrameLayout>
</layout>
